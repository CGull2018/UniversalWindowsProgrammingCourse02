        <TextBox x:ConnectionId='8' Name ="outputBox" TextWrapping="Wrap" HorizontalAlignment="Left" Margin="45,277,0,0" VerticalAlignment="Top" Width="162" Height="112" RenderTransformOrigin="0.935,0.06"/>
        <ListView x:ConnectionId='9' Name ="ListBox" HorizontalAlignment="Left" Height="123" Margin="45,406,0,0" VerticalAlignment="Top" Width="290"/>





    </Grid>
